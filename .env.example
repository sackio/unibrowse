# .env.example - Template for environment variables
# Copy this to .env and fill in your values

# NordVPN Token (optional - for VPN testing features)
# Get your token from: https://my.nordaccount.com/dashboard/nordvpn/
NORDVPN_TOKEN=your_nordvpn_token_here

# VPN Feature Toggle
VPN_ENABLED=false

# MongoDB Connection (defaults to localhost)
MONGODB_URI=mongodb://localhost:27017/browser-mcp

# Server Configuration
PORT=9010
